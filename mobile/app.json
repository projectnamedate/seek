{
  "expo": {
    "name": "Seek",
    "slug": "seek",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "app.seek.mobile",
      "infoPlist": {
        "NSCameraUsageDescription": "Seek needs camera access to capture bounty targets",
        "NSPhotoLibraryUsageDescription": "Seek needs photo library access to save captures"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F172A"
      },
      "package": "app.seek.mobile",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Seek needs camera access to capture bounty targets"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Seek needs photo library access"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "seek-protocol"
      }
    }
  }
}
